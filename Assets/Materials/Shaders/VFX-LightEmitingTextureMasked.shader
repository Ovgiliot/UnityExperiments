				float2 uv_TextureSample0 = IN.ase_texcoord3.xy * _TextureSample0_ST.xy + _TextureSample0_ST.zw;
				float4 tex2DNode8 = tex2D( _TextureSample0, uv_TextureSample0 );
				float4 lerpResult15 = lerp( _Color0 , _Color1 , tex2DNode8.r);
				float3 ase_worldViewDir = ( _WorldSpaceCameraPos.xyz - WorldPosition );
				ase_worldViewDir = normalize(ase_worldViewDir);
				float3 ase_worldNormal = IN.ase_texcoord4.xyz;
				float fresnelNdotV98 = dot( ase_worldNormal, ase_worldViewDir );
				float fresnelNode98 = ( 0.0 + 1.0 * pow( 1.0 - fresnelNdotV98, _FRPow ) );
				float temp_output_104_0 = ( fresnelNode98 * _FRStrenth );
				float4 break13 = saturate( ( ( lerpResult15 * ( tex2DNode8.g * _AlphaContrast ) ) * temp_output_104_0 ) );
				float3 appendResult14 = (float3(break13.r , break13.g , break13.b));
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( appendResult14 * _Intensity );
				float Alpha = break13.a;
				float AlphaClipThreshold = _AlphaClip;
				float AlphaClipThresholdShadow = 0.5;

